{
  "name": "verdaccio-npm-urls",
  "version": "1.0.0",
  "keywords": [
    "npm",
    "package",
    "private",
    "registry",
    "urls",
    "verdaccio"
  ],
  "homepage": "https://github.com/n4bb12/verdaccio-npm-urls/blob/master/README.md",
  "bugs": "https://github.com/n4bb12/verdaccio-npm-urls/issues",
  "repository": "github:n4bb12/verdaccio-npm-urls",
  "license": "MIT",
  "author": "Abraham Schilling",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build node_modules/verdaccio-npm-urls",
    "copy": "cpy dist/* node_modules/verdaccio-npm-urls/",
    "dev": "yarn clean && yarn build && yarn copy && verdaccio -c verdaccio.yml",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "prepack": "yarn build",
    "test": "echo No tests.",
    "update": "yarn clean && yarn build && yarn copy",
    "watch": "nodemon --watch src --ext ts --exec yarn dev"
  },
  "devDependencies": {
    "@n4bb12/config-tslint": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/node": "^12.6.2",
    "cpy-cli": "^2.0.0",
    "express": "^4.17.0",
    "nodemon": "^1.18.10",
    "release-it": "^12.3.3",
    "rimraf": "^3.0.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.2",
    "verdaccio": "^4.3.4"
  },
  "peerDependencies": {
    "express": "4",
    "verdaccio": "3 || 4"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "$schema": "http://json.schemastore.org/package"
}
